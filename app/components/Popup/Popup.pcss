@import '../../styles/variables.pcss';
@import '../../styles/fonts.pcss';

.Popup {
	display: block;
}

.Popup__title {
	background-color: #f3f3f3;
	padding-y: 31px 25px;
	padding-x: 48px;
	font-family: var(--helveticaNeueCyr);
	text-transform: uppercase;
	font-size: 17px;
	font-weight: bold;
	line-height: 1.41;
